<p id="notice"><%= notice %></p>

<h1>Listing Teams</h1>

<table>
  <thead>
    <tr>
      <th>Nickname</th>
      <th>Location</th>
      <th>Team number</th>
      <th>Key</th>
      <th>Images</th>
      <th>Events</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= team.nickname %></td>
        <td><%= team.location %></td>
        <td><%= team.team_number %></td>
        <td><%= team.key %></td>
        <td><%= team.images %></td>
        <td><%= team.events %></td>
        <td><%= link_to 'Show', team %></td>
        <td><%= link_to 'Edit', edit_team_path(team) %></td>
        <td><%= link_to 'Destroy', team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team', new_team_path %>
